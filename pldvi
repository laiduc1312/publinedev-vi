local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()


local MainWindow = Rayfield:CreateWindow({
	Name = "PublineDEV Tool",
	LoadingTitle = "Ducdzz...",
	LoadingSubtitle = "by laiduc1312",
	ConfigurationSaving = {
	   Enabled = true,
	   FolderName = nil, -- Create a custom folder for your hub/game
	   FileName = "PublineDEV Tool"
	},
	Discord = {
	   Enabled = false,
	   Invite = "Discord: https://discord.gg/KqUgYjN9A4", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD.
	   RememberJoins = true -- Set this to false to make them join the discord every time they load it up
	},
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
	   Title = "PublineDEV Tool",
	   Subtitle = "Key System",
	   Note = "Script làm bởi laiduc1312#5735 ",
	   FileName = "Key",
	   SaveKey = true,
	   GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
	   Key = "laiduc13122009"
	}
 })

 local MainTab = MainWindow:CreateTab("Main", 4483362458) -- Title, Image
 local Button = MainTab:CreateButton({
	Name = "Bay",
	Callback = function(v)
		v = print('Successfully')
		loadstring(game:HttpGet"https://raw.githubusercontent.com/laiduc1312/flyv3gui/main/source")()
	end,
 })



 local Button = MainTab:CreateButton({
	Name = "Bàn phím ảo",
	Callback = function(v)
		v = print('Successfully')
		loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
	end,
 })




 local Toggle = MainTab:CreateToggle({
	Name = "Nhảy liên tục",
	CurrentValue = false,
	Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
	Callback = function(InfiniteJumpEnabled)
        local InfiniteJumpEnabled = true
        game:GetService("UserInputService").JumpRequest:connect(function()
            if InfiniteJumpEnabled then
                game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
            end
        end)
	end,
 })



 local Slider = MainTab:CreateSlider({
	Name = "Tộc chạy",
	Range = {16, 250},
	Increment = 10,
	Suffix = "Walkspeed",
	CurrentValue = 10,
	Flag = "Slider1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
	Callback = function(v)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
	end,
 })



 local Slider = MainTab:CreateSlider({
	Name = "Sức mạnh nhảy",
	Range = {50, 500},
	Increment = 10,
	Suffix = "JumpPower",
	CurrentValue = 10,
	Flag = "Slider1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
	Callback = function(v)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
	end,
 })

